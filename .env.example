# Database
DATABASE_URL="postgresql://user:password@localhost:5432/magiterm"

# Supabase (if using)
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
NEXT_PUBLIC_SUPPORT_EMAIL=

# OpenAI
OPENAI_API_KEY=your-openai-api-key

# Agent Models (defaults to gpt-5.1 if not specified)
AGENT_MODEL_EXECUTE=gpt-5.1
AGENT_MODEL_EXPLAIN=gpt-5.1
AGENT_MODEL_SUGGEST=gpt-5.1

# Agent Reasoning Toggle (true/false - defaults to true if not specified)
# Set to 'false' to disable reasoning and use standard model settings
# Note: Only models that support reasoning (e.g., gpt-5.x) should have this enabled
AGENT_REASONING_ENABLED_EXECUTE=true
AGENT_REASONING_ENABLED_EXPLAIN=true
AGENT_REASONING_ENABLED_SUGGEST=true

# Agent Reasoning Effort (low, medium, high - defaults to low if not specified)
# Only applies when AGENT_REASONING_ENABLED_* is true
AGENT_REASONING_EFFORT_EXECUTE=low
AGENT_REASONING_EFFORT_EXPLAIN=low
AGENT_REASONING_EFFORT_SUGGEST=low
